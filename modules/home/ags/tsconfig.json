{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": [ "ES2022" ],
    "allowJs": true,
    "baseUrl": "./",
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": true,
    "typeRoots": [
      "./node_modules/@girs",
      "./types"
    ]
  }
}
